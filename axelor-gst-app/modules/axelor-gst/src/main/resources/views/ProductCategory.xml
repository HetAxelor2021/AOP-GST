<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid name="product-category-grid" model="com.axelor.gst.db.ProductCategory" title="Product Categories"  >
		<field name="name" />
		<field name="code" />
		<field name="gstRate" />
	</grid>
	
	<form name="product-category-form" model="com.axelor.gst.db.ProductCategory" title="Product" >
		<panel title="Product Category Details">
			<field name="name" />
			<field name="code" />
			<field name="gstRate" />
			
		</panel>
	</form>
